{
  "name": "react-lunr",
  "version": "0.2.0",
  "description": "React component to search a Lunr index",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "scripts": {
    "build": "NODE_ENV=production rollup -c",
    "build:watch": "npm run build -- --watch",
    "format": "prettier --write 'src/**/*.js' 'rollup.config.js' 'README.md'",
    "prebuild": "rimraf dist",
    "prepublish": "npm run build"
  },
  "homepage": "https://github.com/angeloashmore/react-lunr",
  "repository": {
    "type": "git",
    "url": "https://github.com/angeloashmore/react-lunr.git"
  },
  "keywords": [
    "react",
    "component",
    "search",
    "lunr"
  ],
  "author": "Angelo Ashmore",
  "license": "MIT",
  "dependencies": {
    "lunr": "^2.3.2",
    "memoize-one": "^4.0.0",
    "prop-types": "^15.6.2"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.52",
    "@babel/plugin-proposal-class-properties": "^7.0.0-rc.2",
    "@babel/preset-env": "^7.0.0-beta.52",
    "babel-core": "^7.0.0-0",
    "prettier": "^1.13.7",
    "rimraf": "^2.6.2",
    "rollup": "^0.62.0",
    "rollup-plugin-babel": "4.0.0-beta.7"
  },
  "peerDependencies": {
    "react": ">=16"
  }
}
